<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns="sap.m">
	<f:SimpleForm class="editableForm" columnsL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="4" id="fmrDiasEstacionales"
		labelSpanL="3" labelSpanM="3" layout="ResponsiveGridLayout" maxContainerCols="1" minWidth="1024">
		<f:content>
			<Label text="{i18n>DesCo}"/>
			<Input name="DesCo" id="DesCo_id" valueLiveUpdate="true" liveChange="_validateSaveEnablement" enabled="true"
				value="{ path: 'DesCo', type: 'sap.ui.model.odata.type.Byte'}"/>
			<Label text="{i18n>DesVe}"/>
			<Input name="DesVe" id="DesVe_id" valueLiveUpdate="true" liveChange="_validateSaveEnablement" enabled="true"
				value="{ path: 'DesVe', type: 'sap.ui.model.odata.type.Byte'}"/>
		</f:content>
	</f:SimpleForm>
</core:FragmentDefinition>